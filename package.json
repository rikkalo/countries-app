    "enzyme-to-json": "^3.4.3",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.3.0",
    "react-test-renderer": "^16.12.0",
    "typescript": "~3.7.2"
